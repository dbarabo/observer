<?xml version="1.0" encoding="windows-1251"?>
<!-- edited with XMLSpy v2011 sp1 (http://www.altova.com) by SAN (FGUP GNIVC FNS Russia) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.00">
	<xs:element name="Файл">
		<xs:annotation>
			<xs:documentation>Файл электронного документа</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ВЫПБНДОПОЛ" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Сведения о счете (специальном банковском счете), ЭСП, вкладе (депозите)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="0">
							<xs:element name="Операции" minOccurs="0" maxOccurs="1000">
								<xs:annotation>
									<xs:documentation>Сведения по операциям на счете (специальном банковском счете), по операциям по вкладу (депозиту), о переводах остатка электронных денежных средств</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="РеквДок">
											<xs:annotation>
												<xs:documentation>Реквизиты документа, на основании которого была совершена операция по счету, вкладу (депозиту), был осуществлен перевод</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="ВидДок" use="optional">
													<xs:annotation>
														<xs:documentation>Вид (шифр) документа</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="11"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:nonNegativeInteger">
															<xs:totalDigits value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомДок" use="required">
													<xs:annotation>
														<xs:documentation>Номер документа, на основании которого была совершена операция, перевод</xs:documentation>
														<xs:documentation>
Дата в формате ДД.ММ.ГГГГ</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="355/23-Т"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="20"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ДатаДок" type="xs:date" use="required">
													<xs:annotation>
														<xs:documentation>Дата документа</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="2010-03-15"/>
																<altova:example value="2013-04-21"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="РеквБанка">
											<xs:annotation>
												<xs:documentation>Реквизиты банка плательщика/получателя денежных средств (ЭСП)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="НомКорСч" use="optional">
													<xs:annotation>
														<xs:documentation>Номер корреспондентского счета банка (иностранного банка)</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="30101810600000000957"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="34"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НаимБП" use="required">
													<xs:annotation>
														<xs:documentation>Наименование банка</xs:documentation>
														<xs:documentation>
Дата в формате ДД.ММ.ГГГГ</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="Банк ВТБ 24"/>
																<altova:example value="Сосьете Женераль"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="160"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="БИКБП" use="required">
													<xs:annotation>
														<xs:documentation>БИК по справочнику БИК России или код банка-нерезидента по SWIFT или национальный код банка</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="046015262"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="34"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="РеквПлат">
											<xs:annotation>
												<xs:documentation>Реквизиты плательщика/получателя денежных средств (ЭСП)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="НаимПП" use="required">
													<xs:annotation>
														<xs:documentation>Наименование организации или ФИО плательщика</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="ОАО Ромашка"/>
																<altova:example value="Депардье Жерар Филипович"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="160"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ИННПП" use="optional">
													<xs:annotation>
														<xs:documentation>ИНН/КИО налогоплательщика</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="5258006800"/>
																<altova:example value="771527131300"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="5"/>
															<xs:maxLength value="12"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="КПППП" use="optional">
													<xs:annotation>
														<xs:documentation>КПП налогоплательщика</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="525801001"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="9"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомСчПП" use="optional">
													<xs:annotation>
														<xs:documentation>Номер счета (специального банковского счета), идентификатор (номер) ЭСП в российском банке или в банке-нерезиденте</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="30101116062230010010"/>
																<altova:example value="40802810500450000070"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="34"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="СуммаОпер">
											<xs:annotation>
												<xs:documentation>Сумма операции (перевода) по счету, вкладу, депозиту (ЭДС)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="Дебет" use="required">
													<xs:annotation>
														<xs:documentation>По дебету (уменьшение)</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="34522.12"/>
																<altova:example value="120345.10"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:decimal">
															<xs:totalDigits value="18"/>
															<xs:fractionDigits value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="Кредит" use="required">
													<xs:annotation>
														<xs:documentation>По кредиту (увеличение)</xs:documentation>
														<xs:appinfo>
															<altova:exampleValues>
																<altova:example value="342.12"/>
																<altova:example value="1203.10"/>
															</altova:exampleValues>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:decimal">
															<xs:totalDigits value="18"/>
															<xs:fractionDigits value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ИдБлок" use="required">
										<xs:annotation>
											<xs:documentation>Идентификатор блока</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="1"/>
													<altova:example value="2"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:nonNegativeInteger">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ДатаОпер" type="xs:date" use="required">
										<xs:annotation>
											<xs:documentation>Дата совершения операции (перевода)</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="2010-03-15"/>
													<altova:example value="2013-04-21"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="НазнПл">
										<xs:annotation>
											<xs:documentation>Назначение платежа или код перевода</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="расходный кассовый ордер"/>
													<altova:example value="1.2"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ПорНом" use="required">
							<xs:annotation>
								<xs:documentation>Порядковый номер счета, ЭСП, вклада (депозита). Соответствует номеру LLLLLL в имени файла</xs:documentation>
								<xs:appinfo>
									<altova:exampleValues>
										<altova:example value="000001"/>
									</altova:exampleValues>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="6"/>
									<xs:pattern value="\d{6}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ПорНомДФ" use="required">
							<xs:annotation>
								<xs:documentation>Порядковый номер дополнительного файла по счету. Соответствует номеру QQQQQQ в имени файла. Для каждого порядкового номера счета начинается с 000001</xs:documentation>
								<xs:appinfo>
									<altova:exampleValues>
										<altova:example value="000001"/>
									</altova:exampleValues>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="6"/>
									<xs:pattern value="\d{6}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="НомСч" use="required">
							<xs:annotation>
								<xs:documentation>Номер счета (специального банковского счета), ЭДС, вклада (депозита)</xs:documentation>
								<xs:appinfo>
									<altova:exampleValues>
										<altova:example value="40702810938090001072"/>
									</altova:exampleValues>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ИдЭС" type="GUIDТип" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор файла</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ТипИнф" use="required">
				<xs:annotation>
					<xs:documentation>Тип информации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="10"/>
						<xs:enumeration value="ВЫПБНДОПОЛ"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ВерсПрог" use="required">
				<xs:annotation>
					<xs:documentation>Версия передающей программы: &lt;наименование&gt;&lt;пробел&gt;&lt;версия&gt;</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="БИНГО 5.20.02"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ТелОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Телефон отправителя</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="(495) 223-33-44"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ДолжнОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Должность отправителя</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="оператор"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="100" fixed="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ФамОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Фамилия отправителя</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Сичкин"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ВерсФорм" use="required">
				<xs:annotation>
					<xs:documentation>Версия формата</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="4"/>
						<xs:fractionDigits value="2"/>
						<xs:enumeration value="3.00"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="АдрРФТип">
		<xs:annotation>
			<xs:documentation>Адрес в Российской Федерации по КЛАДР</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Индекс" use="required">
			<xs:annotation>
				<xs:documentation>Индекс</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="123421"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="6"/>
					<xs:pattern value="[0-9]{6}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="КодРегион" type="ССРФТип" use="required">
			<xs:annotation>
				<xs:documentation>Код Региона по справочнику CCРФ</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="77"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Район" use="optional">
			<xs:annotation>
				<xs:documentation>Район</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Город" use="optional">
			<xs:annotation>
				<xs:documentation>Город</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="Москва"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="НаселПункт" use="optional">
			<xs:annotation>
				<xs:documentation>Населенный пункт</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Улица" use="optional">
			<xs:annotation>
				<xs:documentation>Улица</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="Тверская"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Дом" use="optional">
			<xs:annotation>
				<xs:documentation>Дом</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="25"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Корпус" use="optional">
			<xs:annotation>
				<xs:documentation>Корпус</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="2-Д"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Кварт" use="optional">
			<xs:annotation>
				<xs:documentation>Квартира</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="17-АД"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
