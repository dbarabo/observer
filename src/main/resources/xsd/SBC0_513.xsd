				<altova:exampleValues>
					<altova:example value="7750001495**77500100115040700000006"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="35"/>
			<xs:pattern value="[0-9]{10}[*]{2}[0-9]{23}"/>
			<xs:pattern value="[0-9]{4}[*]{17}[0-9]{14}"/>
		</xs:restriction>
	</xs:simpleType>
