	<xs:element name="Файл">
		<xs:annotation>
			<xs:documentation>Сообщение банка об открытии (закрытии) счета (депозита)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Документ">
					<xs:annotation>
						<xs:documentation>Состав и структура документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвБанк">
								<xs:annotation>
									<xs:documentation>Сведения о банке (филиале банка)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="СвБанкТип"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвНП">
								<xs:annotation>
									<xs:documentation>Сведения об организации, индивидуальном предпринимателе, нотариусе, занимающемся частной практикой, адвокате, учредившем адвокатский кабинет, управляющем товарище инвестиционного товарищества</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="НПРО" type="НПРОТип">
											<xs:annotation>
												<xs:documentation>Российская организация</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="НПИП">
											<xs:annotation>
												<xs:documentation>Индивидуальный предприниматель, нотариус, занимающийся частной практикой, адвокат, учредивший адвокатский кабинет</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="НПИПТип"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="НПИО" type="НПИОТип">
											<xs:annotation>
												<xs:documentation>Иностранная организация</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="НЕТИНН" type="НетИННТип">
											<xs:annotation>
												<xs:documentation>Лицо, сведения об ИНН которого отсутствуют</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
									<xs:attribute name="КодЛица" use="required">
										<xs:annotation>
											<xs:documentation>Код лица</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
												<xs:pattern value="[1-6]{1}"/>
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
												<xs:enumeration value="3"/>
												<xs:enumeration value="4"/>
												<xs:enumeration value="5"/>
												<xs:enumeration value="6"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="СвидНУ" type="СвидТип">
										<xs:annotation>
											<xs:documentation>Свидетельство о постановке на учет в налоговом органе / свидетельство об учете в налоговом органе</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="22,18734451"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвСчет">
								<xs:annotation>
									<xs:documentation>Сведения о счете (депозите)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="Открыт">
											<xs:complexType>
												<xs:attribute name="КодСостСч" use="required">
													<xs:annotation>
														<xs:documentation>Код состояния счета (депозита)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомерДог" use="required">
													<xs:annotation>
														<xs:documentation>Номер договора</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="КодСостДог" use="required">
													<xs:annotation>
														<xs:documentation>Код состояния договора</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="0"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ДатаЗаклДог" type="ДатаТип" use="required">
													<xs:annotation>
														<xs:documentation>Дата заключения договора </xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="Закрыт">
											<xs:complexType>
												<xs:attribute name="КодСостСч" use="required">
													<xs:annotation>
														<xs:documentation>Код состояния счета (депозита)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="0"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ДатаЗакрСч" type="ДатаТип" use="required">
													<xs:annotation>
														<xs:documentation>Дата закрытия счета (депозита)</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="НомерДог" use="optional">
													<xs:annotation>
														<xs:documentation>Номер договора</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="КодСостДог" use="required">
													<xs:annotation>
														<xs:documentation>Код состояния договора</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="1"/>
															<xs:enumeration value="0"/>
															<xs:enumeration value="1"/>
															<xs:enumeration value="2"/>
															<xs:enumeration value="3"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ДатаРастДог" type="ДатаТип">
													<xs:annotation>
														<xs:documentation>Дата расторжения договора</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:choice>
									<xs:attribute name="НомСч" type="НомСчТип" use="required">
										<xs:annotation>
											<xs:documentation>Номер счета (депозита)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ДатаОткрСч" type="ДатаТип" use="required">
										<xs:annotation>
											<xs:documentation>Дата открытия счета (депозита)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="КодСч" use="required">
										<xs:annotation>
											<xs:documentation>Код вида счета (депозита)</xs:documentation>
											<xs:documentation>
Дата в формате ДД.ММ.ГГГГ</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="0001"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="4"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ВалСч" use="required">
										<xs:annotation>
											<xs:documentation>Код валюты счета (депозита)</xs:documentation>
											<xs:appinfo>
												<altova:exampleValues>
													<altova:example value="810"/>
													<altova:example value="A98"/>
												</altova:exampleValues>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ИдДок" type="GUID" use="required">
							<xs:annotation>
								<xs:documentation>Идентификатор документа</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="КНД" use="required">
							<xs:annotation>
								<xs:documentation>Код формы</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="КНДТип">
									<xs:enumeration value="1114301"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="КодНОБ" type="ИФНСТип" use="required">
							<xs:annotation>
								<xs:documentation>Код налогового органа по месту нахождения банка (филиала банка)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="НомСооб" use="required">
							<xs:annotation>
								<xs:documentation>Номер сообщения</xs:documentation>
								<xs:appinfo>
									<altova:exampleValues>
										<altova:example value="148100121400000123"/>
									</altova:exampleValues>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="18"/>
									<xs:pattern value="[0-9]{18}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ТипСооб" use="required">
							<xs:annotation>
								<xs:documentation>Тип сообщения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="3"/>
									<xs:pattern value="[0-9]{3}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ДолжнПрБ" use="required">
							<xs:annotation>
								<xs:documentation>Должность представителя банка</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="100" fixed="false"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ФамПрБ" use="required">
							<xs:annotation>
								<xs:documentation>Фамилия и инициалы представителя банка</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="60"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ТелБанка" use="required">
							<xs:annotation>
								<xs:documentation>Телефон банка</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ДатаСооб" type="ДатаТип" use="required">
							<xs:annotation>
								<xs:documentation>Дата сообщения</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ИдФайл" type="ИдФайлТип" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор файла</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ТипИнф" use="required">
				<xs:annotation>
					<xs:documentation>Тип информации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="10"/>
						<xs:enumeration value="СООБЩБАНКА"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ВерсПрог" use="required">
				<xs:annotation>
					<xs:documentation>Версия передающей программы</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ТелОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Телефон отправителя</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ДолжнОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Должность отправителя</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="100" fixed="false"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ФамОтпр" use="required">
				<xs:annotation>
					<xs:documentation>Фамилия отправителя </xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Иванов"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="КолДок" use="required">
				<xs:annotation>
					<xs:documentation>Количество документов</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:totalDigits value="1"/>
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ВерсФорм" use="required">
				<xs:annotation>
					<xs:documentation>Версия формата</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="5"/>
						<xs:enumeration value="5.13"/>
						<xs:enumeration value="7.77"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="НПРОТип">
		<xs:annotation>
			<xs:documentation>Налогоплательщик – организация</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ИННРО" type="ИННОргТип" use="required">
			<xs:annotation>
				<xs:documentation>ИНН</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="КПП" type="КППТип" use="optional">
			<xs:annotation>
				<xs:documentation>КПП</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ОГРН" type="ОГРНТип" use="optional">
			<xs:annotation>
				<xs:documentation>ОГРН</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="НаимОрг" use="required">
			<xs:annotation>
				<xs:documentation>Полное наименование </xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value='Закрытое акционерное общество "Дезстанция Ромашка"'/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="1000"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="НПИПТип">
		<xs:annotation>
			<xs:documentation>Индивидуальный предприниматель, нотариус, адвокат</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ФИОИП" type="ФИОПрТип">
				<xs:annotation>
					<xs:documentation>Фамилия, имя, отчество </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ИННИП" type="ИННФЛТип" use="required">
			<xs:annotation>
				<xs:documentation>ИНН</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ОГРНИП" type="ОГРНИПТип" use="optional">
			<xs:annotation>
				<xs:documentation>ОГРНИП</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="НПИОТип">
		<xs:annotation>
			<xs:documentation>Иностранная организация</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ИННИО" type="ИННОргТип">
				<xs:annotation>
					<xs:documentation>ИНН</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="КИО" type="КИОТип">
				<xs:annotation>
					<xs:documentation>Код иностранной организации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="НаимОрг" use="required">
			<xs:annotation>
				<xs:documentation>Полное наименование </xs:documentation>
			</xs:annotation>
