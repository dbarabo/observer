					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="250"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ИННКО" type="ИННОргТип" use="required">
			<xs:annotation>
				<xs:documentation>ИНН банка</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="7750001495"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="КППКО" type="КППТип" use="required">
			<xs:annotation>
				<xs:documentation>КПП банка (филиала банка)</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="775001001"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ОГРНКО" type="ОГРНТип" use="required">
