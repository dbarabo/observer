	<xs:simpleType name="ИФНСТип">
		<xs:annotation>
			<xs:documentation>Код ИФНС из Классификатора системы обозначений налоговых органов</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="0512"/>
					<altova:example value="0400"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
			<xs:pattern value="([0-9]{1}[1-9]{1}|[1-9]{1}[0-9]{1})[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
