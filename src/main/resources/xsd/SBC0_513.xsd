				<xs:restriction base="xs:string">
					<xs:minLength value="13"/>
					<xs:maxLength value="15"/>
					<xs:pattern value="[0-9]{13,15}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
