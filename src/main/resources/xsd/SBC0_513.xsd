				<xs:documentation>ОГРН банка</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="1045700167116"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
