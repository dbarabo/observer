				<altova:exampleValues>
					<altova:example value="23,145678342"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="10"/>
			<xs:maxLength value="12"/>
			<xs:pattern value="\d{2},\d{7,9}"/>
