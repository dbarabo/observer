		<xs:attribute name="НомФ" use="required">
			<xs:annotation>
				<xs:documentation>Порядковый номер филиала банка</xs:documentation>
				<xs:appinfo>
					<altova:exampleValues>
						<altova:example value="12"/>
						<altova:example value="0"/>
					</altova:exampleValues>
				</xs:appinfo>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:totalDigits value="4"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="БИК" use="required">
			<xs:annotation>
				<xs:documentation>БИК банка (филиала банка), используемый при расчетах</xs:documentation>
